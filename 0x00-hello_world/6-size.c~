#include <stdio.h>

/**
 *main - print the various data type sizes on -x64 computer
 *
 *Description: the main function prints sizes for char, int
 *long int, long long int and float data types in -x64 computer
 *Return: Always 0 (Success)
 */
int main(void)
{
printf(Size of a char : 1 byte(s)
	 Size of an int : 4 byte(s)
	 Size of a long int : 8 byte(s)
	 Size of a long long int : 8 byte(s)
	 Size of a float : 4 byte(s) );
Return(0);
}
